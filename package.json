{
  "name": "gatsby-source-flickr",
  "description": "A Gatsby source plugin to load resources from the Flickr photo search API.",
  "author": "Chris Searle <chris@chrissearle.org>",
  "repository": "https://github.com/chrissearle/gatsby-source-flickr.git",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "flickr",
    "photos"
  ],
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublishOnly": "yarn build",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "node-fetch": "^2.3.0",
    "query-string": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "cross-env": "^5.2.0",
    "prettier": "^1.15.2"
  },
  "version": "0.0.1"
}
